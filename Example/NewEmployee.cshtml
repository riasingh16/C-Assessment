@model WebApplication4.Models.EmpTable

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>NewEmployee</title>
</head>
<body>
    <h1>New Employee Registeration</h1>
    <div>
        @using (Html.BeginForm())
        {
            <p>
                Enter the EmpID: @Html.EditorFor((m) => m.EmpID)
            </p>
            <p>
                Enter the EmpName: @Html.EditorFor((m) => m.EmpName)

            </p>
            <p>
                Enter the EmpSalary: @Html.EditorFor((m) => m.EmpSalary)
            </p>
            <button>Add Employee</button>
        }
        </div>
</body>
</html>
